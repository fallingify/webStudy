package collection;

import java.util.HashMap;

public class HashMap01 {

	public static void main(String[] args) {
		HashMap<String, Integer> map = new HashMap<>();
		System.out.println(map);
		
		//추가 put()
		map.put("A", 1);
		map.put("B", 2);
		map.put("C", 3);
		map.put("D", 4);
		System.out.println(map);
		

	}

}
